{
    "name": "etna/doctrine-provider",
    "description": "ETNA Doctrine Provider",
    "keywords": ["utils", "config", "provider"],
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "ETNA",
            "email": "dev@etna-alternance.net",
            "homepage": "http://etna-alternance.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\Doctrine\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "TestApp/src/"
        }
    },
    "repositories": [
       {
           "type": "composer",
           "url":  "https://blu-composer.herokuapp.com"
       }
   ],
    "scripts": {
        "test": "behat",
        "behat": "behat",
        "phing": "phing",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "phing/phing": "^2.16",
        "behat/behat": "^3.5",
        "behat/symfony2-extension": "^2.1",
        "etna/composer-behat-utils": "^3.0.0-alpha.1",
        "symfony/orm-pack": "^1.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/console": "^4.2",
        "phpunit/php-code-coverage": "^6.1",
        "phpunit/phpcov": "^5.0",
        "phploc/phploc": "^4.0",
        "squizlabs/php_codesniffer": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.1",
        "php-coveralls/php-coveralls": "^2.1",
        "scrutinizer/ocular": "^1.5",
        "phpstan/phpstan": "^0.11.4",
        "povils/phpmnd": "^2.1",
        "phpmetrics/phpmetrics": "^2.4"
    }
}
