{
    "name": "etna/doctrine-provider",
    "description": "ETNA Doctrine Provider",
    "keywords": ["utils", "config", "provider"],
    "license": "proprietary",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "ETNA",
            "email": "dev@etna-alternance.net",
            "homepage": "http://etna-alternance.net"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "ETNA\\Doctrine\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "TestApp/src"
        }
    },
    "repositories": [
       {
           "type": "composer",
           "url":  "https://blu-composer.herokuapp.com"
       }
   ],
    "scripts": {
        "test": "behat",
        "behat": "behat",
        "phing": "phing",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "phing/phing": "^2.16",
        "behat/behat": "^3.5",
        "behat/symfony2-extension": "^2.1",
        "etna/composer-behat-utils": "^3.0.0-alpha.1",
        "symfony/orm-pack": "^1.0",
        "sensio/framework-extra-bundle": "^5.2"
    }
}
